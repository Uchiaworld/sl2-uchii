f:Box("Teleporte a sv priv","string",function(tpps)
    game.Players.LocalPlayer.startevent:FireServer("teleporttoprivate", tpps)
end)
f:Label("Hecho por Gojo | Version 1.0",{
    TextSize = 15;
    TextColor = Color3.fromRGB(0,0,0); 
    BgColor = Color3.fromRGB(85, 170, 0);
}) 
f:Label("https://discord.gg/cpkvTRsJ5J",{
